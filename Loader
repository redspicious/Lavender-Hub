local gameId = game.GameId

if gameId == 4587615400 then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/redspicious/Lavender-Hub/main/Scripts/Battleborne", true))()
elseif gameId == 110181652 then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/redspicious/Lavender-Hub/main/Scripts/Epic%20Minigames", true))()
elseif gameId == 4234745929 then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/redspicious/Lavender-Hub/main/Scripts/Answer%20or%20Die", true))()
else
	loadstring(game:HttpGet("https://raw.githubusercontent.com/redspicious/Lavender-Hub/main/Scripts/Unsupported", true))()
end
